                <navigator url="../user_ViewGroups/user_ViewGroups" class="weui-grid" hover-class="weui-grid_active">
                    <image class="weui-grid__icon" src="../../style/img/users.svg" />
                    <view class="weui-grid__label">所属群组的查看</view>
                </navigator>
                <navigator url="../user_joinGroups/user_joinGroups" class="weui-grid" hover-class="weui-grid_active">
